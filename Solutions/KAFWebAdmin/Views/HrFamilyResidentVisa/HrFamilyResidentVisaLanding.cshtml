
@{
    ViewBag.Title = KAF.MsgContainer._hr_familyresidentvisa.familyresidentvisaList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrFamilyResidentVisa/HrFamilyResidentVisaNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrFamilyResidentVisa">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrFamilyResidentVisaDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_familyresidentvisa.residencynumber</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyResidentVisaNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyResidentVisaNew">
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyResidentVisaEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyResidentVisaEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyResidentVisaDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyResidentVisaDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrFamilyResidentVisa/LandingHrFamilyResidentVisa.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrFamilyResidentVisa/NewHrFamilyResidentVisa.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrFamilyResidentVisa/EditHrFamilyResidentVisa.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrFamilyResidentVisa/DetailHrFamilyResidentVisa.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



