
@{
    ViewBag.Title = KAF.MsgContainer._hr_civilidinfo.civilidinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrCivilIDInfo/HrCivilIDInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrCivilIDInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrCivilIDInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_civilidinfo.civilidno</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrCivilIDInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrCivilIDInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-HrCivilIDInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrCivilIDInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrCivilIDInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrCivilIDInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrCivilIDInfo/LandingHrCivilIDInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrCivilIDInfo/NewHrCivilIDInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrCivilIDInfo/EditHrCivilIDInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrCivilIDInfo/DetailHrCivilIDInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



