
@{
    ViewBag.Title = KAF.MsgContainer._hr_familycivilidinfo.hr_familycivilidinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrFamilyCivilIDInfo/HrFamilyCivilIDInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrFamilyCivilIDInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrFamilyCivilIDInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_familycivilidinfo.familycivilidno</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyCivilIDInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyCivilIDInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyCivilIDInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyCivilIDInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyCivilIDInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyCivilIDInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFamilyCivilIDInfo/LandingHrFamilyCivilIDInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFamilyCivilIDInfo/NewHrFamilyCivilIDInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFamilyCivilIDInfo/EditHrFamilyCivilIDInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFamilyCivilIDInfo/DetailHrFamilyCivilIDInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



